  "bugs": {
    "url": "https://github.com/grecoww/API-creation-demo/issues"
  },
  "homepage": "https://github.com/grecoww/API-creation-demo#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/module-alias": "^2.0.2",
    "@types/node": "^20.7.0",
    "@types/supertest": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
